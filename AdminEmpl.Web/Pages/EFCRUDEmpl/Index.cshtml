@page
@model AdminEmpl.Web.Pages.EFCRUDEmpl.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Empleado[0].Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Empleado[0].CorreoE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Empleado[0].FotoDir)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Empleado[0].Depart)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Empleado) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CorreoE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FotoDir)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Depart)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
